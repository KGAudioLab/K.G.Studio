{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "/@fs/*": "${webRoot}/*",
                "webpack:///./~/*": "${webRoot}/node_modules/*",
                "webpack://?:*/*": "${webRoot}/*"
            },
            "preLaunchTask": "npm: dev"
        }
    ]
}